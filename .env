# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json
GOOGLE_CLOUD_REGION=us-central1

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_MODEL=gpt-4

# API Keys for External Services
GOOGLE_CUSTOM_SEARCH_API_KEY=your-google-custom-search-key
GOOGLE_CUSTOM_SEARCH_ENGINE_ID=your-search-engine-id

# Bot Tokens
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
DISCORD_WEBHOOK_URL=your-discord-webhook-url

# Reddit API (Optional)
REDDIT_CLIENT_ID=your-reddit-client-id
REDDIT_CLIENT_SECRET=your-reddit-client-secret
REDDIT_USER_AGENT=your-app-name

# Application Settings
APP_ENV=development
DEBUG=true
LOG_LEVEL=INFO

# Agent Configuration
MAX_PARALLEL_AGENTS=5
AGENT_TIMEOUT=300
MAX_TOKENS=4000

# Database Configuration (Firestore)
FIRESTORE_COLLECTION_INTAKE=intake_requests
FIRESTORE_COLLECTION_WORKFLOWS=workflows
FIRESTORE_COLLECTION_PROFILES=user_profiles

# Web Scraping
PLAYWRIGHT_HEADLESS=true
SCRAPING_TIMEOUT=30

# Hugging Face (Optional)
HUGGINGFACE_API_TOKEN=your-huggingface-token

# Apify (Optional)
APIFY_API_TOKEN=your-apify-token
